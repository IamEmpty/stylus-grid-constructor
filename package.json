{
  "name": "stylus-grid-constructor",
  "version": "1.1.0",
  "description": "CSS grid constructor, written in stylus",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm test",
    "build": "mkdir -p build && stylus test/grid.styl -m -o build",
    "watch": "mkdir -p build && stylus -w test/grid.styl -m -o build",
    "dist": "stylus src/grid.styl -s -o dist/grid.css",
    "clean": "rm -rf build node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IamEmpty/stylus-grid-constructor.git"
  },
  "keywords": [
    "stylus",
    "grid"
  ],
  "author": "IamEmpty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IamEmpty/stylus-grid-constructor/issues"
  },
  "homepage": "https://github.com/IamEmpty/stylus-grid-constructor#readme",
  "devDependencies": {
    "pug": "^2.0.0-alpha4",
    "stylus": "^0.54.2"
  }
}
